PKGFOLDER=~/.hmDict
tPage="https://www.moneydj.com/Z/ZA/ZAA/ZAA.djhtm"
exeName=$0
exeName=${exeName##*/}
exeName=${exeName%.*}
exeName=$exeName.py
python3 $PKGFOLDER/$exeName -v $tPage
#hint:For batch file transformation
